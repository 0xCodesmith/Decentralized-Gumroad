{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "pages/**/*.ts",
    "components/**/*.tsx"
, "pages/index.tsx", "pages/_document.tsx", "pages/_app.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
